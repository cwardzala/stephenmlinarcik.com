			{{#videos}}
				<div class="vimeography-slide">

					<iframe id="vimeography-embed-{{gallery_id}}-{{id}}" src="https://player.vimeo.com/video/{{id}}?title=0&byline=0&portrait=0&api=1&player_id=vimeography-embed-{{gallery_id}}-{{id}}" width="{{width}}" height="{{height}}" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe>

					<div class="vimeography-data">
						<h1>{{name}}</h1>
						<p>{{{short_description}}}</p>
					</div>
				</div>

			{{/videos}}
